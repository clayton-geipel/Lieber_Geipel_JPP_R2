@Article{TanahashiMurakamiChoiEtAl2005,
  author   = {Mamoru Tanahashi and Shinichirou Murakami and Gyung-Min Choi and Yuichi Fukuchi and Toshio Miyauchi},
  title    = {Simultaneous {CH}-{OH} {PLIF} and Stereoscopic {PIV} Measurements of Turbulent Premixed Flames},
