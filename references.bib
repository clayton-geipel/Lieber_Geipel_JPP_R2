@Article{StellaGujKompenhansEtAl2001,
  author     = {Stella, A. and Guj, G. and Kompenhans, J. and Raffel, M. and Richard, H.},
  title      = {Application of Particle Image Velocimetry to Combusting Flows: Design Considerations and Uncertainty Assessment},
