@Article{OsborneRamjiCarterEtAl2016,
  author   = {Osborne, Jeffrey R. and Ramji, Sarah A. and Carter, Campbell D. and Peltier, Scott and Hammack, Stephen and Lee, Tonghun and Steinberg, Adam M.},
  title    = {Simultaneous 10 {kHz} {TPIV, OH PLIF, and CH2O PLIF} measurements of turbulent flame structure and dynamics},
